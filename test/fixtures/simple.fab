<fabula>
module.exports = {
  default: {
    branch: 'my-branch',
    someFlag: true
  }
}
</fabula>

<commands>
cd ~
local mkdir -p foobar
local git checkout <%= branch %>
git checkout <%= branch %>-2
local echo "foobarfobar" > foobar

<% if (someFlag) { %>
local touch /tmp/some-file
<% } %>
</commands>
